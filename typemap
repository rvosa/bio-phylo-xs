TYPEMAP
const char *    T_PV
Node *          O_OBJECT
Tree *          O_OBJECT
Listable *      O_OBJECT
Writable *      O_OBJECT
Identifiable *  O_OBJECT
U64             T_UV
U8              T_UV
U8 *            T_PV

INPUT
O_OBJECT
	if ( SvTRUE($arg) ) {
    	$var = ($type)SvIV(SvRV($arg));		
	}
	else {
    	$var = NULL;
    }

OUTPUT
O_OBJECT
	if ( $var == NULL ) {
		$arg = NULL;
	}
	else {
		sv_setref_pv( $arg, ((Identifiable*)$var)->_class, (void*)$var );
	}
