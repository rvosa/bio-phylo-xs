INPUT
O_OBJECT
	if ( SvTRUE($arg) ) {
		$var = ($type)SvIV(SvRV($arg));
	}
	else {
		$var = NULL;
	}

OUTPUT
O_OBJECT
	if ( $var == NULL ) {
		$arg = NULL;
	}
	else {
		int idx = ((Identifiable*)$var)->_index;
		sv_setref_pv( $arg, package[idx], (void*)$var );
	}
